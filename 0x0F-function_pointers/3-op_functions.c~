#include "3-calc.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * add - adds int a and b
 * @a: first integer 
 * @b: second integer
 * Return: sum of a and b
 */
int op_add(int a, int b);
{
  return (a + b);
}

/**
 * add - adds int a and b
 * @a: first integer
 * @b: second integer
 * Return: sum of a and b
 */

int op_sub(int a, int b);
{
  return (a - b);
}



int op_mul(int a, int b);
{
  return (a * b);
}

int op_div(int a, int b);
{
  return (a / b);
}


int op_mod(int a, int b);
{
  return (a % b);
}
